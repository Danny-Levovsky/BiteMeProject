<AnchorPane prefHeight="417.0" prefWidth="654.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ceo.CEOController">
   <children>
      <Label fx:id="txtCEOName" layoutX="20.0" layoutY="14.0" onDragDetected="#updateName" text="CEO Name" />
      <Button id="btnExit" fx:id="btnLogout" layoutX="22.0" layoutY="31.0" mnemonicParsing="false" onAction="#getBtnLogout" text="Logout" />
      <Label layoutX="252.0" layoutY="82.0" text="CEO  Menu">
         <font>
            <Font name="System Bold" size="29.0" />
         </font></Label>
      <Button id="btnExit" fx:id="BtnViewQuarterReports" layoutX="348.0" layoutY="244.0" mnemonicParsing="false" onAction="#getBtnViewQuarterReports" text="View Quarter Reports" />
      <Button id="btnExit" fx:id="BtnViewBranchReports" layoutX="503.0" layoutY="169.0" mnemonicParsing="false" onAction="#getBtnViewBranchReports" text="View Branch Reports" />
      <ComboBox fx:id="branchComboBox" layoutX="153.0" layoutY="169.0" prefWidth="150.0" promptText="choose branch" />
      <ComboBox fx:id="monthComboBox" layoutX="328.0" layoutY="169.0" prefWidth="150.0" promptText="choose month" />
      <Label layoutX="49.0" layoutY="173.0" text="Branch reports:" />
      <Label layoutX="49.0" layoutY="248.0" text="Quarter reports:" />
