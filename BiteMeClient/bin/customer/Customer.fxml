<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="255.0" prefWidth="429.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CustomerController">
   <children>
      <Label fx:id="txtCustomerName" layoutX="10.0" layoutY="6.0" onDragDetected="#updateName" text="Customer Name" />
      <Button id="btnExit" fx:id="btnLogout" layoutX="26.0" layoutY="23.0" mnemonicParsing="false" onAction="#getBtnLogout" text="Logout" />
      <Label layoutX="113.0" layoutY="66.0" text="Customer Menu">
         <font>
            <Font name="System Bold" size="29.0" />
         </font></Label>
      <Button id="btnExit" fx:id="BtnNewOrder" layoutX="266.0" layoutY="149.0" mnemonicParsing="false" onAction="#getBtnNewOrder" text="New Order" />
      <Button id="btnExit" fx:id="BtnViewOrder" layoutX="97.0" layoutY="149.0" mnemonicParsing="false" onAction="#getBtnViewOrder" text="View Order" />
   </children>
</AnchorPane>
