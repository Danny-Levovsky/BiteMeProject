<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="437.0" prefWidth="621.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NewOrderController">
   <children>
      <ComboBox fx:id="restaurantComboBox" layoutX="181.0" layoutY="33.0" prefWidth="150.0" promptText="choose restaurant" />
      <TableView layoutX="156.0" layoutY="97.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Label layoutX="28.0" layoutY="325.0" text="item number:" />
      <Button fx:id="btnBack" layoutX="21.0" layoutY="378.0" mnemonicParsing="false" onAction="#getBtnBack" text="Back" />
      <TextField fx:id="txt" layoutX="121.0" layoutY="321.0" />
      <Button fx:id="btnAddItem" layoutX="343.0" layoutY="321.0" mnemonicParsing="false" onAction="#getBtnAddItem" text="add item" />
      <Button fx:id="btnFinish" layoutX="497.0" layoutY="378.0" mnemonicParsing="false" onAction="#getBtnFinish" text="finish" />
   </children>
</AnchorPane>
